{
    "name": "manager-client",
    "private": true,
    "scripts": {
        "start": "turbo start",
        "build": "pnpm moveBuild",
        "moveBuild": "pnpm workspace @scm/build-utils postbuild",
        "dev": "turbo dev",
        "lint": "turbo lint",
        "lint:fix": "turbo lint -- --fix",
        "format": "prettier --write \"**/*.{ts,tsx,md}\""
    },
    "devDependencies": {
        "@scm/build-utils": "*",
        "prettier": "^3.2.4",
        "turbo": "^1.11.3"
    },
    "engines": {
        "node": ">=18"
    },
    "packageManager": "pnpm@8.14.1",
    "workspaces": [
        "apps/*",
        "packages/*"
    ]
}
