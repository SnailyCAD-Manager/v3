{
    "name": "manager-client",
    "private": true,
    "scripts": {
        "build": "turbo build && yarn moveBuild",
        "moveBuild": "yarn workspace @scm/build-utils postbuild",
        "dev": "turbo dev",
        "lint": "turbo lint",
        "lint:fix": "turbo lint -- --fix",
        "format": "prettier --write \"**/*.{ts,tsx,md}\""
    },
    "devDependencies": {
        "@scm/build-utils": "*",
        "prettier": "^3.2.4",
        "turbo": "^1.11.3"
    },
    "engines": {
        "node": ">=18"
    },
    "packageManager": "yarn@1.22.19",
    "workspaces": [
        "apps/*",
        "packages/*"
    ]
}
